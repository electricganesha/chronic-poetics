{
  "extends": ["prettier", "next", "next/core-web-vitals"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/jsx-props-no-spreading": "off",
    "@next/next/no-img-element": "off"
  },
  "env": {
    "es6": true
  }
}
